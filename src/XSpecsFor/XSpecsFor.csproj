<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.0.1</Version>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageProjectUrl>https://github.com/fs7744/XSpecsFor</PackageProjectUrl>
    <PackageLicenseUrl>https://raw.githubusercontent.com/fs7744/XSpecsFor/master/LICENSE</PackageLicenseUrl>
    <Copyright>copyright @2017 Victor.X.Qu</Copyright>
    <Authors>Victor.X.Qu</Authors>
    <Company>Victor.X.Qu</Company>
    <Description>BDD test format like SpecsFor for xunit.net</Description>
    <PackageReleaseNotes>BDD test format like SpecsFor for xunit.net</PackageReleaseNotes>
    <AssemblyVersion>0.0.1.0</AssemblyVersion>
    <FileVersion>0.0.1.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="2.0.0" />
    <PackageReference Include="xunit" Version="2.2.0" />
  </ItemGroup>

</Project>
